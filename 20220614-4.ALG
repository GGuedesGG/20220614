Algoritmo "20220614-4"

Var
   n, c, a : inteiro
Inicio
      c := 0
      a := 0

      enquanto (c < 10) faca
         escreval("Digite um número inteiro qualquer:")
         leia(n)
         c := c + 1
         a:= a + n
      fimenquanto
      
      escreval("A soma dos números lidos é:", a)
Fimalgoritmo